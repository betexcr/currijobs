    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "test:api": "node scripts/test-api-simple.js",
    "test:database": "node scripts/test-api-simple.js",
    "test:zod": "node scripts/validate-schemas.js",
    "validate:schemas": "node scripts/validate-schemas.js",
    "validate:types": "tsc --noEmit",
    "check:types": "npm run validate:types",
    "generate:types": "echo 'TypeScript types are generated from Zod schemas'",
    "test": "npm run test:api && npm run test:zod && npm run validate:types",
    "test:watch": "echo 'Tests temporarily disabled - focusing on manual testing'",
