  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:lan": "cross-env EXPO_PUBLIC_USE_LOCAL_DB=true EXPO_PUBLIC_USE_SUPABASE=true EXPO_PUBLIC_POSTGREST_URL=http://$LAN_IP:3000 expo start --host lan",
